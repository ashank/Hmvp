<?xml version="1.0" encoding="utf-8"?>
<!-- 全键盘-26-字母 -->
<Keyboard
    bg_res="@drawable/main_bg"
    key_bg_res="@drawable/key_bg_rectangle"
    key_height="@dimen/px100"
    key_left_padding="@dimen/px10"
    key_press_res="@drawable/softkey_bg_press2"
    key_select_res="@drawable/softkey_bg_select2"
    key_text_color="#b8bbbf"
    key_text_size="@dimen/px60"
    key_top_padding="@dimen/px10"
    key_width="@dimen/px100"
    start_pos_x="@dimen/px240"
    start_pos_y="@dimen/px20" >

    <!-- 第一行 -->
    <row>
        <keys
            codes="8|9|10|11|12|13|14|15|16|7"
            labels="1|2|3|4|5|6|7|8|9|0"
            splitter="|" />

        <key
            key_icon="@drawable/ju_hao_e"
            key_label="." />
        <!-- 删除 -->
        <key
            key_code="67"
            key_icon="@drawable/delete" />
        <!-- 删除 -->
        <key
            key_code="4"
            key_icon="@drawable/quit_skb" />
    </row>

    <!-- 第二行 -->
    <row>

        <!-- 切换回英文键盘 -->
        <toggle_key
            key_code="@string/mode_skb_all_en"
            key_label="abc"
            key_text_size="@dimen/px40" >
            <state
                key_label="返回"
                key_text_size="@dimen/px30"
                state_id="@string/mode_skb_all_en" />
        </toggle_key>
        <toggle_key key_label="，" >
            <state
                key_label="，"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="{"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="。" >
            <state
                key_label="。"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="}"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="!" >
            <state
                key_label="!"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="["
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="\?" >
            <state
                key_label="\?"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="]"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label=":" >
            <state
                key_label=":"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="&lt;"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label=";" >
            <state
                key_label=";"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label=">"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="&quot;" >
            <state
                key_label="“"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="《"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="&quot;" >
            <state
                key_label="”"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="》"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="*" >
            <state
                key_label="*"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="_"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="(" >
            <state
                key_label="("
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="|"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label=")" >
            <state
                key_label=")"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="`"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <!-- 回车 -->
         <key
            key_code="@string/keycode_center"
            key_icon="@drawable/g_search_icon"
            key_height="@dimen/px210"
            key_text_size="@dimen/px30" />
    </row>

    <!-- 第三行 -->
    <row key_bottom_padding="@dimen/px30" >
        <toggle_key
            key_code="@string/toggle_keycode_page"
            key_label="翻页"
            key_text_size="@dimen/px30" >
             <state
                key_label="翻页"
                key_text_size="@dimen/px30"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="翻页1"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        
        <toggle_key
            key_label=".com"
            key_text_size="@dimen/px30" >
            <state
                key_label=".com"
                key_text_size="@dimen/px30"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="+"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key
            key_label=".CN"
            key_text_size="@dimen/px30" >
            <state
                key_label=".CN"
                key_text_size="@dimen/px30"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="-"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key
            key_label="www."
            key_text_size="@dimen/px30" >
            <state
                key_label="www."
                key_text_size="@dimen/px30"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="="
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="/" >
            <state
                key_label="/"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="%"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <!-- 空格 -->
        <key
            key_code="@string/keycode_space"
            key_icon="@drawable/kongge"
            key_width="@dimen/px210" />

        <toggle_key key_label="`" >
            <state
                key_label="`"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="\#"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="~" >
            <state
                key_label="~"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="￥"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <toggle_key key_label="..." >
            <state
                key_label="…"
                state_id="@string/toggle_keycode_page_1" />
            <state
                key_label="\@"
                state_id="@string/toggle_keycode_page_2" />
        </toggle_key>
        <!-- 光标向左移动 -->
        <key
            key_code="@string/keycode_left"
            key_icon="@drawable/key_move_left"/>
        <!-- 光标向右移动 -->
        <key
            key_code="@string/keycode_right"
            key_icon="@drawable/key_move_right" />
    </row>

</Keyboard>